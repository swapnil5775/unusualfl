{
  "version": 2,
  "builds": [
    {
      "src": "seasonality.py",
      "use": "@vercel/python"
    },
    {
      "src": "seasonality_etf.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/seasonality",
      "dest": "seasonality.py"
    },
    {
      "src": "/seasonality/per-ticker",
      "dest": "seasonality.py"
    },
    {
      "src": "/seasonality/etf-market",
      "dest": "seasonality_etf.py"
    },
    {
      "src": "/api/ai-summary",
      "dest": "seasonality_etf.py"
    },
    {
      "src": "/(.*)",
      "dest": "seasonality.py"
    }
  ],
  "env": {
    "PORT": "5000"
  }
}
