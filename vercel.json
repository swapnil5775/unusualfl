{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/seasonality",
      "dest": "app.py"
    },
    {
      "src": "/seasonality/per-ticker",
      "dest": "app.py"
    },
    {
      "src": "/seasonality/etf-market",
      "dest": "app.py"
    },
    {
      "src": "/api/ai-summary",
      "dest": "app.py"
    },
    {
      "src": "/etf-research",
      "dest": "app.py"
    },
    {
      "src": "/institution/list",
      "dest": "app.py"
    },
    {
      "src": "/research",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PORT": "5000"
  },
  "excludeFiles": [
    "__pycache__/**",
    "*.log",
    "*.txt"
  ]
}
